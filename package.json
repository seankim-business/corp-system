{
  "name": "kyndof-corp-system",
  "version": "1.0.0",
  "description": "Multi-tenant SaaS with Google Workspace integration and AI agent orchestration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc || true",
    "start": "node dist/index.js",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:load": "tsx tests/load/multi-account-stress.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "multi-tenant",
    "saas",
    "google-workspace",
    "oauth",
    "ai-agents",
    "typescript",
    "express",
    "prisma"
  ],
  "author": "Kyndof Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@bull-board/api": "^6.16.4",
    "@bull-board/express": "^6.16.4",
    "@bull-board/ui": "^6.16.4",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@notionhq/client": "^5.8.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/sdk-trace-base": "^2.5.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@prisma/client": "^6.19.2",
    "@prisma/instrumentation": "^6.19.2",
    "@sentry/node": "^10.36.0",
    "@sentry/profiling-node": "^10.36.0",
    "@slack/bolt": "^4.6.0",
    "@types/cron": "^2.4.3",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.67.1",
    "bullmq-otel": "^1.1.0",
    "chrono-node": "^2.9.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^4.4.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^9.6.0",
    "googleapis": "^131.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "limiter": "^3.0.0",
    "openai": "^6.16.0",
    "redis": "^5.10.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "playwright": "^1.58.0",
    "prettier": "^3.2.4",
    "prisma": "^6.19.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
